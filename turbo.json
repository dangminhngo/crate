{
  "$shema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "env": [
        "NODE_ENV",
        "DATABASE_URL",
        "PORT",
        "CLIENT_ORIGIN_URL",
        "AUTH0_AUDIENCE",
        "AUTH0_DOMAIN",
        "AUTH0_MANAGEMENT_API_TOKEN"
      ]
    },
    "dev": { "cache": false, "persistent": true },
    "lint": {},
    "db:generate": {
      "cache": false
    },
    "db:push": {
      "cache": false
    }
  }
}
