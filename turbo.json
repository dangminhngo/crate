{
  "$shema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "outputs": [
        "dist/**",
        ".next/**",
        "!.next/cache/**",
        "build/**",
        "api/**",
        "public/build/**"
      ],
      "dependsOn": ["^build"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false
    },
    "lint": {},
    "ts:check": {
      "cache": false
    },
    "test": {},
    "db:generate": {
      "cache": false
    },
    "db:push": {
      "cache": false
    },
    "db:seed": {
      "cache": false
    }
  },
  "globalDependencies": [".env"],
  "globalEnv": [
    "NODE_ENV",
    "CI",
    "PLAYWRIGHT_TEST_BASE_URL",
    "PLAYWRIGHT_HEADLESS",
    "DATABASE_URL",
    "PORT",
    "CLIENT_ORIGIN_URL",
    "AUTH0_AUDIENCE",
    "AUTH0_DOMAIN",
    "AUTH0_MANAGEMENT_API_TOKEN"
  ],
  "globalDotEnv": [".env.local", ".env"]
}
