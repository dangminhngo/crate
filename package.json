  "devDependencies": {
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@ianvs/prettier-plugin-sort-imports": "^3.7.2",
    "@types/jest": "^29.5.1",
    "eslint": "^8.39.0",
    "eslint-config-custom": "workspace:*",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "turbo": "^1.9.3"
  },
  "packageManager": "pnpm@8.3.1",
  "dependencies": {
    "dotenv-cli": "^7.2.1"
  }
