{
  "name": "crate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "dotenv -e .env.local turbo dev",
    "start": "turbo start",
    "clean": "rm -rf node_modules && rm -rf out && turbo clean",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "test": "turbo test",
    "test:e2e": "start-server-and-test dev http://localhost:3000 \"playwright test\"",
    "test:dev": "start-server-and-test dev http://localhost:3000 test",
    "test:start": "start-server-and-test start http://localhost:3000 test",
    "db:generate": "turbo db:generate",
    "db:push": "turbo db:push",
    "db:seed": "turbo db:seed",
    "prepare": "husky install",
    "postinstall": "pnpm db:generate"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@ianvs/prettier-plugin-sort-imports": "^3.7.2",
    "@playwright/test": "^1.34.3",
    "@types/jest": "^29.5.1",
    "@types/node": "^18.16.3",
    "eslint": "^8.39.0",
    "eslint-config-custom": "workspace:*",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-mock-extended": "^3.0.4",
    "prettier": "^2.8.8",
    "start-server-and-test": "^2.0.0",
    "ts-jest": "^29.1.0",
    "turbo": "^1.9.9"
  },
  "packageManager": "pnpm@8.3.1",
  "dependencies": {
    "dotenv-cli": "^7.2.1"
  }
}
